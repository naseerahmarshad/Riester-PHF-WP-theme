{
  "env": {
    "browser": true,
    "es2022": true,
    "jquery": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "warn"
  },
  "globals": {
    "wp": "readonly",
    "jQuery": "readonly"
  }
}
