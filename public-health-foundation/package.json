{
  "name": "public-health-foundation",
  "version": "1.0.0",
  "description": "Public Health Foundation WordPress theme - Child theme of Frost",
  "scripts": {
    "start": "npm run watch",
    "watch": "npx npm-run-all --parallel watch:sass watch:js",
    "watch:sass": "npx npm-run-all --parallel watch:sass:*",
    "watch:sass:style": "sass --watch src/sass/phf-styles.scss:style.css --style=expanded",
    "watch:sass:editor": "sass --watch src/sass/editor.scss:assets/css/editor-styles.css --style=expanded",
    "watch:js": "esbuild src/js/main.js --bundle --outfile=assets/js/phf-scripts.js --watch",
    "build": "npx npm-run-all clean build:*",
    "build:sass": "npx npm-run-all --parallel build:sass:*",
    "build:sass:style": "sass src/sass/phf-styles.scss:style.css --style=expanded --no-source-map",
    "build:sass:editor": "sass src/sass/editor.scss:assets/css/editor-styles.css --style=expanded --no-source-map",
    "build:js": "esbuild src/js/main.js --bundle --minify --outfile=assets/js/phf-scripts.min.js",
    "clean": "rimraf assets/js/phf-scripts*.js",
    "lint:js": "eslint src/js/**/*.js",
    "lint:scss": "stylelint \"src/sass/**/*.scss\"",
    "lint": "npx npm-run-all --parallel lint:*"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
